@using System.Text.Json.Serialization
@inherits BaseIonicInputComponent<int, IonRange.InputChangeEventDetail>
@namespace Blazor.Ionic
<ion-range dual-knobs="false" value="@Value" @attributes="InputAttributes" @ref="Element">@ChildContent</ion-range>

@code {

    public class InputChangeEventDetail : BaseIonicChangeEventDetail<int>
    {
        [JsonPropertyName("value")]
        public int Value { get; set; }

        public override int GetValue()
        {
            return Value;
        }
    }
}